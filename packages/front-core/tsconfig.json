{
  "extends": "../../tsconfig.json",
  "compilerOptions": {
    "target": "es2019",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "declaration": true,
    "composite": true,
    "outDir": "./dist",
    "allowJs": true,
    "skipLibCheck": true,
    "module": "esnext",
    "isolatedModules": true,
    "noEmit": true,
    "jsx": "react"
  },
  "include": [
    "src"
  ],
  "exclude": [
    "dist/**/*"
  ]
}
