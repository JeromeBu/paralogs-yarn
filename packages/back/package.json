{
  "name": "@paralogs/back",
  "version": "0.0.1",
  "private": true,
  "main": ".build/src/index.js",
  "types": ".build/src/index.d.ts",
  "scripts": {
    "deploy": "sls config credentials --provider aws --key $AWS_ACCESS_KEY_ID --secret $AWS_SECRET_KEY && sls deploy",
    "unit-test": "jest",
    "test:watch": "yarn unit-test --watchAll",
    "integration-test": "jest â€”c jest.config.integration.js",
    "ci:test": "yarn unit-test --watchAll=false;",
    "build": "tsc",
    "build:watch": "tsc --watch",
    "start": "node dist/adapters/primaries/express/start.js",
    "dev": "ts-node src/adapters/primaries/express/start.ts",
    "start:dev": "nodemon",
    "startServerless": "sls dynamodb install; sls offline start;",
    "precommit": "lint-staged",
    "create-migration":  "knex migrate:make --knexfile ~/code/JeromeBu/paralogs/packages/back/src/adapters/secondaries/repositories/postGres/knex/knexfile.ts",
    "migrate:latest": "knex migrate:latest --knexfile ~/code/JeromeBu/paralogs/packages/back/src/adapters/secondaries/repositories/postGres/knex/knexfile.ts",
    "migrate:rollback": "knex migrate:rollback --all --knexfile ~/code/JeromeBu/paralogs/packages/back/src/adapters/secondaries/repositories/postGres/knex/knexfile.ts"
  },
  "dependencies": {
    "@paralogs/shared": "*",
    "@types/yup": "^0.26.32",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "faker": "^4.1.0",
    "jsonwebtoken": "^8.5.1",
    "knex": "^0.20.11",
    "lodash": "^4.17.15",
    "morgan": "^1.9.1",
    "pg": "^7.18.2",
    "serverless": "^1.59.3",
    "serverless-plugin-monorepo": "^0.8.0",
    "supertest": "^4.0.2",
    "yup": "^0.28.3"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.35",
    "@types/bcryptjs": "^2.4.2",
    "@types/cors": "^2.8.6",
    "@types/express": "^4.17.2",
    "@types/faker": "^4.1.9",
    "@types/jest": "24.0.23",
    "@types/jsonwebtoken": "^8.3.6",
    "@types/lodash": "^4.14.147",
    "@types/morgan": "^1.7.37",
    "@types/pg": "^7.14.1",
    "@types/supertest": "^2.0.8",
    "@types/yup": "^0.26.32",
    "aws-sdk": "^2.570.0",
    "jest": "24.9.0",
    "lint-staged": "^9.4.2",
    "serverless-dynamodb-local": "^0.2.38",
    "serverless-offline": "^5.12.0",
    "supertest": "^4.0.2",
    "ts-jest": "^24.1.0",
    "ts-node": "^8.6.2",
    "typescript": "3.8.3",
    "yup": "^0.28.3"
  },
  "lint-staged": {
    "src/**/*.{ts,tsx,js}": [
      "eslint --config ../../.eslintrc.commit.js --fix",
      "git add"
    ]
  },
  "nodemonConfig": {
    "ignore": [
      "**/*.test.ts",
      "**/*.spec.ts",
      ".git",
      "node_modules"
    ],
    "watch": [
      "src"
    ],
    "exec": "npm run dev",
    "ext": "ts"
  },
  "workspaces": {
    "nohoist": [
      "jest",
      "jest/**",
      "@types/jest/**",
      "@types/jest",
      "lint-staged",
      "lint-staged/**"
    ]
  }
}
