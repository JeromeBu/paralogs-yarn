{
  "extends": "../../tsconfig.json",
  "compilerOptions": {
    "target": "es2017",
    "lib": ["esnext"],
    "forceConsistentCasingInFileNames": true,
    "module": "commonjs",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "outDir": ".build",
    "rootDir": "./src",
    "resolveJsonModule": true,
    "typeRoots": ["./customTypings", "./node_modules/@types"]
  },
  "include": ["src", "customTypings"],
  "exclude": [".build"]
}
